<p id="notice"><%= notice %></p>

<h1>Courses</h1>

<table>
  <thead>
    <tr>
      <th style="padding-right: 10px">Section</th>
      <th style="padding-right: 10px">Course</th>
      <th style="padding-right: 10px">Professor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td style="padding-right: 10px"><%= course.sectionNumber %></td>
        <td style="padding-right: 10px"><%= course.courseNumber %></td>
        <td style="padding-right: 10px"><%= course.professor %></td>
        <td style="padding-right: 10px"><%= link_to 'Show', course %></td>
        <td style="padding-right: 10px"><%= link_to 'Edit', edit_course_path(course) %></td>
        <td style="padding-right: 10px"><%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_course_path %>
